{
  "default": {
    "paths": ["test/**/*.feature"],
    "requireModule": ["ts-node/register"],
    "baseURL": "https://yoigo.com",
    "require": ["test/**/*.ts"],
    "reports": "test/reports",
    "browser": "chrome",
    "tags": "@only and not @ignore",
    "reportHTML": true,
    "reportName": "QA Suite project nama",
    "displayDuration": true,
    "durationInMS": false,
    "timeout": 30000
  },
  "ci": {
    "paths": ["test/**/*.feature"],
    "requireModule": ["ts-node/register"],
    "baseURL": "https://yoigo.com",
    "require": ["test/**/*.ts"],
    "reports": "test/reports",
    "browser": "chrome:headless",
    "tags": "not @ignore",
    "reportHTML": true,
    "reportName": "QA Suite project nama",
    "displayDuration": true,
    "durationInMS": false,
    "parallel": 2,
    "retry": 1
  }
}
